							"    pm.expect(jsonData).to.have.property('material');",
							"    pm.expect(jsonData).to.have.property('price');",
							"    pm.collectionVariables.set('handbag_id', jsonData.handbagId);",
							"});",
							"",
							"pm.test(\"Created handbag has correct data\", function () {",
							"    var jsonData = pm.response.json();",
							"    pm.expect(jsonData.modelName).to.eql('Elegant#2024');",
							"    pm.expect(jsonData.material).to.eql('Leather');",
							"    pm.expect(jsonData.price).to.eql(250.5);",
