		{
			"name": "1. Login_Pass",
			"event": [
				{
					"listen": "test",
					"script": {
						"exec": [
							"pm.test(\"Status code is 200\", function () {",
							"    pm.response.to.have.status(200);",
							"});",
							"",
							"pm.test(\"Response has token and role\", function () {",
							"    var jsonData = pm.response.json();",
							"    pm.expect(jsonData).to.have.property('token');",
							"    pm.expect(jsonData).to.have.property('role');",
							"    pm.collectionVariables.set('jwt_token', jsonData.token);",
							"});",
							"",
							"pm.test(\"Role is valid\", function () {",
							"    var jsonData = pm.response.json();",
							"    pm.expect(['Administrator', 'Moderator', 'Developer', 'Member']).to.include(jsonData.role);",
							"});"
						],
						"type": "text/javascript",
						"packages": {}
					}
				}
			],
			"request": {
				"method": "POST",
				"header": [
					{
						"key": "Content-Type",
						"value": "application/json"
					}
				],
				"body": {
					"mode": "raw",
					"raw": "{\n  \"email\": \"admin@leopard.com\",\n  \"password\": \"@1\"\n}"
				},
				"url": {
					"raw": "{{base_url}}/api/auth",
