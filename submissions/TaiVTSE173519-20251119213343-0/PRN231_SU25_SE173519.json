		{
			"name": "Login succes",
			"event": [
				{
					"listen": "test",
					"script": {
						"exec": [
							"if (pm.response && pm.response.code === 200) {\r",
							"    let jsonData;\r",
							"    try {\r",
							"        jsonData = pm.response.json();\r",
							"    } catch (e) {\r",
							"        console.log(\"Response is not valid JSON:\", e);\r",
							"    }\r",
							"\r",
							"    if (jsonData) {\r",
							"        pm.test(\"Response has token\", function () {\r",
							"            pm.expect(jsonData.token).to.exist;\r",
							"        });\r",
							"\r",
							"        pm.test(\"Response has role\", function () {\r",
							"            pm.expect(jsonData.role).to.exist;\r",
							"        });\r",
							"    }\r",
							"} else {\r",
							"    console.log(\"Response is missing or status is not 200\");\r",
							"}\r",
							""
						],
						"type": "text/javascript",
						"packages": {}
					}
				}
