{
  "openapi": "3.0.1",
  "info": {
    "title": "Your API",
    "version": "v1"
  },
  "paths": {
    "/api/auth": {
      "post": {
        "tags": [
          "LeopardAccounts"
        ],
        "requestBody": {
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/LoginRequestDTO"
              }
            },
            "text/json": {
              "schema": {
                "$ref": "#/components/schemas/LoginRequestDTO"
              }
            },
            "application/*+json": {
              "schema": {
                "$ref": "#/components/schemas/LoginRequestDTO"
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "OK"
          }
        }
      }
    },
    "/api/LeopardProfile": {
      "get": {
        "tags": [
          "LeopardProfiles"
        ],
        "responses": {
          "200": {
            "description": "OK"
          }
        }
      },
      "post": {
        "tags": [
          "LeopardProfiles"
        ],
        "requestBody": {
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/LeopardProfileDTO"
              }
            },
            "text/json": {
              "schema": {
                "$ref": "#/components/schemas/LeopardProfileDTO"
              }
            },
            "application/*+json": {
              "schema": {
                "$ref": "#/components/schemas/LeopardProfileDTO"
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "OK"
          }
        }
      }
    },
    "/api/LeopardProfile/{id}": {
      "get": {
        "tags": [
          "LeopardProfiles"
        ],
        "parameters": [
          {
            "name": "id",
            "in": "path",
            "required": true,
            "schema": {
              "type": "integer",
              "format": "int32"
            }
          }
        ],
        "responses": {
          "200": {
            "description": "OK"
          }
        }
      },
      "put": {
        "tags": [
          "LeopardProfiles"
        ],
        "parameters": [
          {
            "name": "id",
            "in": "path",
            "required": true,
            "schema": {
              "type": "integer",
              "format": "int32"
            }
          }
        ],
        "requestBody": {
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/LeopardProfileDTO"
              }
            },
            "text/json": {
              "schema": {
                "$ref": "#/components/schemas/LeopardProfileDTO"
              }
            },
            "application/*+json": {
              "schema": {
                "$ref": "#/components/schemas/LeopardProfileDTO"
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "OK"
          }
        }
      },
      "delete": {
        "tags": [
          "LeopardProfiles"
        ],
        "parameters": [
          {
            "name": "id",
            "in": "path",
            "required": true,
            "schema": {
              "type": "integer",
              "format": "int32"
            }
          }
        ],
        "responses": {
          "200": {
            "description": "OK"
          }
        }
      }
    },
    "/api/LeopardProfile/search": {
      "get": {
        "tags": [
          "LeopardProfiles"
        ],
        "parameters": [
          {
            "name": "LeopardName",
            "in": "query",
            "schema": {
              "type": "string"
            }
          },
          {
            "name": "Weight",
            "in": "query",
            "schema": {
              "type": "number",
              "format": "double"
            }
          }
        ],
        "responses": {
          "200": {
            "description": "OK"
          }
        }
      }
    }
  },
  "components": {
    "schemas": {
      "LeopardProfileDTO": {
        "required": [
          "careNeeds",
          "characteristics",
          "leopardName",
          "weight"
        ],
        "type": "object",
        "properties": {
          "leopardTypeId": {
            "type": "integer",
            "format": "int32",
            "nullable": true
          },
          "leopardName": {
            "minLength": 1,
            "pattern": "^([A-Z0-9][a-zA-Z0-9#]*\\s)*([A-Z0-9][a-zA-Z0-9#]*)$",
            "type": "string"
          },
          "weight": {
            "minimum": 16,
            "type": "number",
            "format": "double"
          },
          "characteristics": {
            "minLength": 1,
            "type": "string"
          },
          "careNeeds": {
            "minLength": 1,
            "type": "string"
          }
        },
        "additionalProperties": false
      },
      "LoginRequestDTO": {
        "type": "object",
        "properties": {
          "email": {
            "type": "string",
            "nullable": true
          },
          "password": {
            "type": "string",
            "nullable": true
          }
        },
        "additionalProperties": false
      }
    },
    "securitySchemes": {
      "Bearer": {
        "type": "http",
        "description": "Please enter the token in the following format: Bearer {token}",
        "scheme": "Bearer",
        "bearerFormat": "JWT"
      }
    }
  },
  "security": [
    {
      "Bearer": [ ]
    }
  ]
}