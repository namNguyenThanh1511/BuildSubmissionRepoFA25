		{
			"name": "Login success",
			"event": [
				{
					"listen": "test",
					"script": {
						"exec": [
							"pm.test(\"Request method is POST\", function () {\r",
							"    pm.expect(pm.request.method).to.eql(\"POST\");\r",
							"});\r",
							"\r",
							"pm.test(\"Request body has email and password\", function () {\r",
							"    var requestBody = JSON.parse(pm.request.body.raw);\r",
							"    pm.expect(requestBody).to.have.property(\"email\");\r",
							"    pm.expect(requestBody).to.have.property(\"password\");\r",
							"});\r",
							"\r",
							"pm.test(\"Status code is 200\", function () {\r",
							"    pm.response.to.have.status(200);\r",
							"});\r",
							"\r",
							"pm.test(\"Login success and save JWT\", function () {\r",
							"    var jsonData = pm.response.json();\r",
							"    pm.expect(jsonData.token).to.be.a(\"string\");\r",
							"    pm.expect(jsonData.token).to.not.be.empty;\r",
							"    pm.environment.set(\"jwt_token\", jsonData.token);\r",
							"});"
						],
						"type": "text/javascript",
						"packages": {}
					}
				}
			],
