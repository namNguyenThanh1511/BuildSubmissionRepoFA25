	"item": [
		{
			"name": "1. Login Success",
			"event": [
				{
					"listen": "test",
					"script": {
						"exec": [
							"// Test 1: Login Success",
							"pm.test('Status code is 200', function () {",
							"    pm.response.to.have.status(200);",
							"});",
							"",
							"pm.test('Response contains token', function () {",
							"    const responseJson = pm.response.json();",
							"    pm.expect(responseJson).to.have.property('token');",
							"    pm.expect(responseJson.token).to.not.be.empty;",
							"    ",
							"    // Store JWT token for subsequent requests",
							"    pm.collectionVariables.set('jwtToken', responseJson.token);",
							"    console.log('JWT Token stored:', responseJson.token.substring(0, 50) + '...');",
							"});",
							"",
							"pm.test('Response contains role', function () {",
							"    const responseJson = pm.response.json();",
							"    pm.expect(responseJson).to.have.property('role');",
							"    const validRoles = ['administrator', 'moderator', 'developer', 'member'];",
							"    pm.expect(validRoles).to.include(responseJson.role);",
							"    console.log('User role:', responseJson.role);",
							"});",
							"",
							"pm.test('Response message validation', function () {",
							"    const responseJson = pm.response.json();",
							"    pm.expect(responseJson.token).to.match(/^[A-Za-z0-9-_=]+\\.[A-Za-z0-9-_=]+\\.[A-Za-z0-9-_.+/=]*$/);",
							"});"
						],
						"type": "text/javascript",
						"packages": {}
					}
				}
			],
